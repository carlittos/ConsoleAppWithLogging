{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "Path": "/logs/consoleappwithlogging.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 365
        }
      }
    ]
  }
}
